<div id="delete-save-state-warn-modal" class="modal fade" role="dialog" tabindex="-1">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">{{ translator.translate('save.delete.title') }}</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="{{ translator.translate('aria.close') }}">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body">
        {{ translator.translate('save.delete.warning', {'__NAME__': '<b id="delete-save-state-name"></b>'}) | raw }}
      </div>
      <div class="modal-footer">
        <button id="delete-save-state" type="button" class="btn btn-danger" data-dismiss="modal" data-save-state-id="">
          {{ translator.translate('delete') }}
        </button>
        <button id="cancel-delete-save-state" type="button" class="btn btn-default" data-dismiss="modal">
          {{ translator.translate('cancel') }}
        </button>
      </div>
    </div>
  </div>
</div>
