{% verbatim %}
  <script id="buff-progress-template" type="text/template">
    <div id="{{ id }}" class="buff-progress">
      <div class="buff-label">{{ buffText }}</div>
      <div class="progress">
        <div class="progress-bar"
             role="progressbar"
             aria-valuenow="100"
             aria-valuemin="0"
             aria-valuemax="100"
             style="width:100%"
        >
        </div>
      </div>
    </div>
  </script>
{% endverbatim %}
